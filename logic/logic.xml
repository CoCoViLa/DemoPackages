  <class>
    <name>And</name>
    <description>And</description>
    <icon>and.gif</icon>
    
    <graphics>
      <bounds x="0" y="0" width="50" height="50" />
      <arc x="-50" y="0" width="100" height="50" startAngle="270" arcAngle="180" colour="16777215"
      filled="true" />
      <arc x="-50" y="0" width="100" height="50" startAngle="270" arcAngle="180" colour="0"
      filled="false" />
      <line x1="0" y1="0" x2="0" y2="50" colour="0" />
    </graphics>

    <ports>
      <port name="in1" type="int" x="0" y="15" portConnection="area" strict="false" />
      <port name="in2" type="int" x="0" y="35" portConnection="area" strict="false" />
      <port name="out" type="int" x="50" y="25" portConnection="area" strict="false" />
    </ports>
    <fields>
      <field name="in1" type="int" description="First input">
        <known>
          <graphics>
            <text string="*self" colour="-16777216" x="-10" y="8" fontname="Arial" fontstyle="1"
            fontsize="10" />
          </graphics>
        </known>
      </field>
      <field name="in2" type="int">
        <known>
          <graphics>
            <text string="*self" colour="-16777216" x="-10" y="50" fontname="Arial" fontstyle="1"
            fontsize="10" />
          </graphics>
        </known>
      </field>
      <field name="out" type="int">
        <known>
          <graphics>
            <text string="*self" colour="-16777216" x="60" y="25" fontname="Arial" fontstyle="1"
            fontsize="10" />
          </graphics>
        </known>
      </field>
    </fields>
