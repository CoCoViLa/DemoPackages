<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE package SYSTEM "package2.dtd">
<package>
	<name>UML Class diagram</name>
	<description>A package for drawing UML class diagrams</description>	
	<class>
		<name>UMLClass</name>
