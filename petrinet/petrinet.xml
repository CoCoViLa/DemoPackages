<line x1="50rf" y1="0" x2="40rf" y2="-5" colour="-16777216" fixed="false" stroke="1" linetype="0" transparency="255"/>
<line x1="50rf" y1="0" x2="40rf" y2="5" colour="-16777216" fixed="false" stroke="1" linetype="0" transparency="255"/>
</graphics>
